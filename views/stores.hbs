{{#extends 'base'}}

{{#block 'content'}}
<table class="table">
    <thead>
        <tr>
            <th>Store ID</th>
            <th>Store Address</th>
            <th>Supervisor</th>
        </tr>
    </thead>
    {{#each stores}}
    <tr>
        <td>{{this.store_id}}</td>
        <td>{{this.address}} {{this.address2}}</td>
        <td>{{this.first_name}} {{this.last_name}}</td>
    </tr>
    {{/each}}
</table>

{{/block}}

{{/extends}}